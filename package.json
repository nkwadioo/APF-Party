{
  "name": "apf-party",
  "version": "0.1.5",
  "scripts": {
    "ng": "ng",
    "serve": "node server/index.js",
    "start": "ng serve",
    "build": "ng build",
    "build:docs": "ng build --base-href /AFP-Party/",
    "build:dist": "ng build --base-href ./",
    "build:github-pages": "ng build --configuration production --output-path docs --base-href https://nkwadioo.github.io/AFP-Party/",
    "watch": "ng build --watch --configuration development",
    "build:prod:dist": "ng build --configuration production --base-href ./",
    "build:artifact": "mkdir deploy-artifact && cp -r ./server/ deploy-artifact &&  cp -r ./dist/APF-Party deploy-artifact/server/www",
    "install:artifact:dependencies": "cd deploy-artifact/server && npm install",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "rxjs": "~7.8.0",
    "swiper": "^10.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.1.3"
  }
}
